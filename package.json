{
  "name": "alan",
  "version": "1.0.0",
  "description": "My website",
  "main": "index.js",
  "scripts": {
    "start": "npm install && node index.js",
    "dev": "NODE_ENV=development node index.js",
    "scss": "node-sass --output-style compressed -o public/dist/css public/src/scss",
    "autoprefixer": "postcss -u autoprefixer -r public/dist/css/*",
    "lint": "eslint public/src/js",
    "babelify": "mkdir -p public/dist/js && babel public/src/js --out-dir public/dist/js",
    "ugilfy": "uglifyjs public/dist/js/*.js -m -o public/dist/js/*.js",
    "imagemin": "imagemin public/src/images public/dist/images -p",
    "build:css": "npm run scss && npm run autoprefixer",
    "build:js": "npm run lint && npm run babelify && npm run uglify",
    "watch:css": "onchange 'public/src/scss/*.scss' -- npm run build:css",
    "watch:js": "onchange 'public/src/js/*.js' -- npm run build:js",
    "watch:all": "parallelshell 'npm run watch:css' 'npm run watch:js'",
    "development": "parallelshell 'npm run dev' 'npm run watch:all'"
  },
  "author": "Alan Chandler",
  "repository": "https://github.com/chandlera/alan",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.12.0",
    "browser-sync": "^2.9.11",
    "compression": "^1.6.1",
    "errorhandler": "^1.3.4",
    "express": "^4.12.2",
    "express3-handlebars": "^0.5.2",
    "helmet": "^0.12.0",
    "jshint": "^2.8.0",
    "morgan": "^1.5.1",
    "throng": "^1.0.0"
  },
  "engines": {
    "node": "0.12.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.2.0",
    "eslint-plugin-react": "^4.0.0",
    "imagemin-cli": "^2.1.0",
    "letsencrypt": "^1.3.0",
    "node-sass": "^3.4.2",
    "onchange": "^2.0.0",
    "parallelshell": "^2.0.0",
    "path": "^0.12.7",
    "postcss-cli": "^2.5.1",
    "sass-loader": "^3.1.2",
    "uglify-js": "^2.6.1",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
